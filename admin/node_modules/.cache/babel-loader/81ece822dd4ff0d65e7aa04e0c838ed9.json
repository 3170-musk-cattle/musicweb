{"ast":null,"code":"export default {\n  name: 'Test',\n\n  data() {\n    return {\n      input: '',\n      artistData: []\n    };\n  },\n\n  created() {\n    this.showArtists();\n  },\n\n  methods: {\n    showArtists() {\n      this.axios.get('http://127.0.0.1:8000/api/show_artists').then(response => {\n        console.log(response.data.list);\n        this.artistData = response.data.list;\n      }).catch(function (error) {\n        console.log(error);\n      });\n    },\n\n    operationRow(row) {\n      console.log(row.pk);\n      this.axios.get('api/delete_artist', {\n        params: {\n          pk: row.fields.name\n        }\n      }).then(response => {\n        console.log(response.data.msg);\n        this.showArtists();\n      }).catch(function (error) {\n        console.log(error);\n      });\n    },\n\n    addArtist() {\n      this.axios.get('api/add_artist', {\n        params: {\n          name: this.input\n        }\n      }).then(response => {\n        console.log(response.data.msg);\n        this.showArtists();\n      }).catch(function (error) {\n        console.log(error);\n      });\n      this.input = '';\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAqBA,eAAe;AACbA,MAAI,EAAE,MADO;;AAEbC,MAAG,GAAK;AACN,WAAO;AACLC,WAAK,EAAE,EADF;AAELC,gBAAU,EAAE;AAFP,KAAP;AAID,GAPY;;AAQbC,SAAM,GAAK;AACT,SAAKC,WAAL;AACD,GAVY;;AAWbC,SAAO,EAAE;AACPD,eAAU,GAAK;AACb,WAAKE,KAAL,CAAWC,GAAX,CAAe,wCAAf,EACGC,IADH,CACSC,QAAD,IAAc;AAClBC,eAAO,CAACC,GAAR,CAAYF,QAAQ,CAACT,IAAT,CAAcY,IAA1B;AACA,aAAKV,UAAL,GAAkBO,QAAQ,CAACT,IAAT,CAAcY,IAAhC;AACD,OAJH,EAKGC,KALH,CAKS,UAAUC,KAAV,EAAiB;AAAEJ,eAAO,CAACC,GAAR,CAAYG,KAAZ;AAAoB,OALhD;AAMD,KARM;;AASPC,gBAAW,CAAGC,GAAH,EAAQ;AACjBN,aAAO,CAACC,GAAR,CAAYK,GAAG,CAACC,EAAhB;AACA,WAAKX,KAAL,CAAWC,GAAX,CAAe,mBAAf,EAAoC;AAAEW,cAAM,EAAE;AAAED,YAAE,EAAED,GAAG,CAACG,MAAJ,CAAWpB;AAAjB;AAAV,OAApC,EACGS,IADH,CACSC,QAAD,IAAc;AAClBC,eAAO,CAACC,GAAR,CAAYF,QAAQ,CAACT,IAAT,CAAcoB,GAA1B;AACA,aAAKhB,WAAL;AACD,OAJH,EAKGS,KALH,CAKS,UAAUC,KAAV,EAAiB;AAAEJ,eAAO,